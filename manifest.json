{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "icons": {
        "16": "/images/water.png",
        "32": "/images/water.png",
        "48": "/images/water.png",
        "128": "/images/water.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "/images/water.png",
            "32": "/images/water.png",
            "48": "/images/water.png",
            "128": "/images/water.png"
        }
    },
    "options_page": "options.html",
    "background": {
        "service_worker": "background.js"
    }
}